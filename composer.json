{
  "name": "zanphp/http-foundation",
  "require": {
    "php": ">=5.6",
    "zanphp/contracts": "dev-master",
    "zanphp/container": "dev-master",
    "zanphp/coroutine": "dev-laogui",
    "zanphp/exception": "dev-master",
    "zanphp/support": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\HttpFoundation\\": "src/"
    },
    "files": [
      "src/Command/Task.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}